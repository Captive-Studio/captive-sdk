{
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "json",
    "yaml",
    "vue"
  ],
  "stylelint.enable": true,
  "stylelint.validate": ["css", "scss", "vue"],
  "css.validate": false,
  "scss.validate": false
}
